/* Smartphones  ----------- */
@media only screen 
and (min-width : 480px) {


.landing{
	min-height: 560px;
}


#center-fog{
	img{
		height: 100%;
	}
}

.chandelier{
	height: auto;
	margin: -200px auto 0;
}

.ornament{
	margin: -21px auto 18px;
}

/*picasa slideshow*/
.picasa-wrapper{
	width: 320px;
	margin: 0 auto;
	height: 250px;
}

.picasagallery{
	height: 250px;
	width: 300px;
	margin: 0 auto;

	.pic{
		width: 500px;
	}
}

.video-content{
	width: 410px;
	margin: 0 auto;
}

}



@media only screen 
and (min-width: 580px) {

.landing{
	min-height: 660px;
}

.chandelier{
	height: auto;
	margin: -250px auto 0;
}
}


/* iPads (portrait and landscape) ----------- */
@media only screen 
and (min-width : 750px) {

// .landing{
// 	height: 760px;
// }

.page:nth-child(odd){
	background: url(/images/zwartevilt.png) $bg-grey;
}

.ornament{
	margin: -27px auto 18px;
}


/*picasa slideshow*/
.picasa-wrapper{
	position: relative;
	// z-index: 5000;
	width: 620px;
	margin: 0 auto;
	height: 400px;
	overflow: hidden;

	img#prevbtn{
		opacity: .8;
		position: absolute;
		left: 0;
		margin-top: 160px;
		display: inline-block;
		float: left;
	}

	img#nextbtn{
		opacity: .8;
		position: absolute;
		right: 0;
		margin-top: 160px;
		display: inline-block;
		float: right;
	}
}

.picasagallery{
	height: 300px;
	clear: both;
	width: 500px;
	margin: 0 auto;

	.pic{
		width: 500px;
		display: inline-block;
		text-align: center;
	}
}


.contact .page-inner{
	// overflow: hidden;
	width: 770px;
	margin: 0 auto;
}

.form{
	float: left;
}

.contact-info{
	display: block;
	float: right;
	width: 225px;
	margin: 18px 0 0 80px;

	a{
		font-size: 3em;
		font-family: 'Tangerine', cursive;
	}
}


footer{
	p{
		float: none;
		text-align: center;
		margin: 0 auto;
		width: 100%;
		display: block;
		height: 54px;
	}
}
}/*min-width 750*/









//adjusts header nav after JS removes slide in menu
@media only screen 
and (min-width : 769px) {

.landing{
	min-height: 760px;
}

header, footer{

	h1{
		float: left;
	}

	nav{
		display: inline-block;
		float: right;
		padding-right: 18px;
		padding-top: 18px;
		width: 300px;

		ul{
			li{
				display: inline-block;
				height: 44px;
				text-align: center;
				margin-right: 4%;
				border: none;

				:hover{
					color: $red;
				}

				a{
					font-size: 2.1em;
				}
			}
		}
	}
}


footer{
	nav{
		ul{
			li{
				a{
					font-size: 1.6em;
					color: $white;
				}
			}
		}
	}
}
.menu-button{
	display: none;
}

// footer{
// 	border-top: 1px solid $red;

// 	a{
// 		color: $white;
// 	}
