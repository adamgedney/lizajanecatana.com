*/
/*--------------------------------Media Queries------------------------*/





/* Smartphones  ----------- */
@media only screen 
and (min-width : 480px) {

.scene{
	height: 360px;
}

.ornament{
	margin: -21px auto 18px;
}

.video-content{
	width: 410px;
	margin: 0 auto;
}

}









/* iPads (portrait and landscape) ----------- */
@media only screen 
and (min-width : 750px) {

.scene{
	height: 590px;
}

#center-fog{
	height: 620px;

	img{
		height: 620px;
	}
}

.ornament{
	margin: -27px auto 18px;
}


/*picasa slideshow*/
.picasa-wrapper{
	position: relative;
	// z-index: 5000;
	width: 620px;
	margin: 0 auto;
	height: 400px;
	overflow: hidden;

	img#prevbtn{
		opacity: .8;
		position: absolute;
		left: 0;
		margin-top: 160px;
		display: inline-block;
		float: left;
	}

	img#nextbtn{
		opacity: .8;
		position: absolute;
		right: 0;
		margin-top: 160px;
		display: inline-block;
		float: right;
	}
}

.picasagallery{
	height: 300px;
	clear: both;
	width: 500px;
	margin: 0 auto;

	.pic{
		width: 500px;
		display: inline-block;
		text-align: center;
	}
}


.contact .page-inner{
	// overflow: hidden;
	width: 770px;
	margin: 0 auto;
}

.form{
	float: left;
}

.contact-info{
	display: block;
	float: right;
	width: 225px;
	margin: 18px 0 0 80px;

	a{
		font-size: 3em;
		font-family: 'Tangerine', cursive;
	}
}


footer{
	p{
		float: none;
		text-align: center;
		margin: 0 auto;
		width: 100%;
		display: block;
		height: 54px;
	}
}
}/*min-width 750*/









//adjusts header nav after JS removes slide in menu
@media only screen 
and (min-width : 769px) {

header{
	position: fixed;
	top: 0;
	left: 0;
	right: 0;
	z-index: 99999;

	-webkit-box-shadow: 2px 2px 3px $rich-black;
	   -moz-box-shadow: 2px 2px 3px $rich-black;
			box-shadow: 2px 2px 3px $rich-black;
}

header, footer{

	h1{
		float: left;
	}

	nav{
		display: inline;
		float: right;
		padding-right: 18px;
		padding-top: 18px;
		width: 300px;

		ul{
			li{
				display: inline-block;
				height: 44px;
				text-align: center;
				margin-right: 4%;
				border: none;

				:hover{
					color: $red;
				}

				a{
					font-size: 2.1em;
				}
			}
		}
	}
}


footer{
	nav{
		ul{
			li{
				a{
					font-size: 1.6em;
					color: $white;
				}
			}
		}
	}
}
.menu-button{
	display: none;
}

// footer{
// 	border-top: 1px solid $red;

// 	a{
// 		color: $white;
// 	}
// }

}









/* Desktops and laptops ----------- */
@media only screen 
and (min-width : 837px) {

.scene{
	height: 620px;
}

.scene-inner{
	position: absolute;
	max-height: 297px;
	left: 13%;
	right: 13%;
	top: 0;

}

.chandelier{
	width: 862px;
	height: 620px;
	margin: -235px auto 0;
	position: absolute;

}

#curtain-wrapper{
	display: block;
}

.page{
	padding-top: 108px;
	padding-bottom: 108px;
}

.landing.page{
	padding-top: 18px;
	padding-bottom: 0;
}

.ornament{
	margin: -117px auto 90px;
}

.bio{
	.two-col {
       -moz-column-count: 2;
       -moz-column-gap: 20px;
       -webkit-column-count: 2;
       -webkit-column-gap: 20px;
	}
}

.videos{

	.video-content{
		width: 720px;
		margin: 0 auto;
	}

	.video-wrapper{
		float: left;
		

		iframe{
		width: 320px;
		height: 233px; 
		}
	}

	.video-wrapper:nth-child(odd){
			margin: 0 4%;
		}
}


}






@media only screen 
and (min-width : 960px) {



/*add CSS columns here for bio text*/
