/*Baseline Grid*/
/*    3px    */

/*----Sass Palette----*/
$black: #101010;
$rich-black: #000;
$light-grey: #9c9c9c;
$mid-grey: #363636;
$grey: #7f7f7f;
$dark-grey: #252525;
$bg-grey: #171717;
$white: #e6e6e6;
$blue: #1e89b1;
$purple: #AB14B4;
$red: #cd241b;
$dark-red: #490406;

/*----Sass imports----*/
@import "reset";
@import "defaults";







/*-----------------------Styles------------------------*/
.menu-button{
	display: block;
	position: fixed;
	right: 9px;
	top: 18px;
	opacity: .8;
	z-index: 99999;
}

header{
	// padding: 2%;
	border-top: 3px solid $dark-red;
	overflow: hidden;
	background: $rich-black;
	height: 63px;
	

	h1{
		text-align: center;
		font-size: 4.2em;
		padding-left: 2%;
		color: $white;
		line-height: 63px;
	}

	nav{
		ul{
			li{
				height: 44px;
				border-top: 1px solid $dark-grey;
				text-align: center;

				:hover{
					color: $red;
				}

				a{
					color: $white;
					line-height: 36px;
					font-size: 1.2em;
				}
			}
		}
	}
}

.landing{
	width: 100%;
	
	.page-inner{
		padding: 0;

	}
}

/* ----------------Landing page scene----------------*/

// .scene-overflow{
// 	overflow: hidden;
// 	height: 297px;
// 	margin-bottom: 36px;
// }

.scene-overflow{
	position: relative;
}

.scene{
	height: 297px;
	position: relative;
	overflow: hidden;
	margin-top: 36px;
	margin-bottom: 0;

	.scene-inner{
		width: 862px;
		top: 0;
	}

	img{
		// width: 100%;
		position: absolute;
		top: 0;
		display: block;
		height: auto;
	}
}

#curtain-wrapper{
	width: 100%;
	min-width: 837px;
	min-height: 100%;
	position: absolute;
	top: 0;
	z-index: 10;
	display: none;
}


#curtain-left, #fog-left{
	position: absolute;
	left: 0;
}

#curtain-right, #fog-right{
	position: absolute;
	right: 0;
}

#curtain-center{
	position: absolute;
	z-index: -1;
	display: block;
	min-width: 100%;
	min-height: 620px;
	margin: 0 auto;
	background: url(../images/scene/8c-curtainCenter.png) repeat;
}

#center-fog{
	position: absolute;
	width: 100%;
	top: 0;
	left: 0;
	right: 0;
	// z-index: 100000;

	img{
		width: 100%;
		position: absolute;
		display: block;
		top: 0;
		height: 297px;
	}
}

.chandelier{
	width: 392px;
	height: 250px;
	// margin: 0 auto;
	margin: -435px auto 0;
	position: absolute;

}

//css swing taken from
//http://iamdavesmith.com/swinging-an-element-using-css3-transitions-and-a-smidgen-of-jquery/
.swing {
	-webkit-transition: all 5s ease-in-out;
	-moz-transition: all 5s ease-in-out;
	-o-transition: all 5s ease-in-out;
	-ms-transition: all 5s ease-in-out;
	transition: all 5s ease-in-out;
	-moz-transform:rotate(2deg);
	-webkit-transform:rotate(2deg);
	-o-transform:rotate(2deg);
	-ms-transform:rotate(2deg);
}
 
.right {
	-moz-transform:rotate(0deg);
	-webkit-transform:rotate(0deg);
	-o-transform:rotate(0deg);
	-ms-transform:rotate(0deg);
}

.light{
	z-index: 9999;
}


/*-------------End scene---------------*/
.page{
	// border-bottom: 1px solid $dark-grey;
	padding: 2% 0;

	-webkit-box-shadow: -2px -2px 5px $rich-black;
	   -moz-box-shadow: -2px -2px 5px $rich-black;
			box-shadow: -2px -2px 5px $rich-black;
}

.page:nth-child(odd){
	background: url(/images/zwartevilt.png) $bg-grey;
	-webkit-box-shadow: inset 2px 2px 5px $rich-black;
	   -moz-box-shadow: inset 2px 2px 5px $rich-black;
			box-shadow: inset 2px 2px 5px $rich-black;
}

.page-inner{
	padding: 0 6%;

}

.ornament{
	display: block;
	width: 150px;
	max-height: 19px;
	margin: -18px auto 18px;
	text-align: center;
	position: relative;
	z-index: 99999;
}

h2{
	font-size: 6.3em;
	line-height: .8em;
	margin-top: 72px;
	margin-bottom: 18px;
	text-align: center;
}

.line-sep{
	display: inline-block;
	height: 18px;
	width: 100%;	
}

.picasagallery img{
	-webkit-box-shadow: 2px 2px 5px $rich-black;
	   -moz-box-shadow: 2px 2px 5px $rich-black;
			box-shadow: 2px 2px 5px $rich-black;
}

.videos{
	// overflow: hidden;
	// max-width: 420px;
	// margin: 0 auto;

	.page-inner{
		// overflow: hidden;
	}

	.video-content{
		overflow: hidden;
	}

	.video-wrapper{
		float: left;
		margin-right: 2%;

		iframe{
		display: inline-block;
		text-align: center;
		width: 420px;
		height: 315px; 
		}

		h3{
			text-align: center;
			margin-top: 9px;
			margin-bottom: 36px;
		}
	}
}

/*picasa thumb display*/
// .picasa{
// 	img{
// 		max-width: 320px;
// 		margin: 0 2% 2%;
// 	}
// }

/*picasa slideshow*/
.picasa-wrapper{
	position: relative;
	// z-index: 5000;
	width: 320px;
	margin: 0 auto;
	height: 250px;
	overflow: hidden;

	img#prevbtn{
		display: none;
	}

	img#nextbtn{
		display: none;
	}
}

.picasagallery{
	height: 250px;
	clear: both;
	width: 300px;
	margin: 0 auto;

	.pic{
		width: 500px;
		display: inline-block;
		text-align: center;
	}
}


/*---------------- Contact form----------------*/
.form{
	width: 294px;
	margin: 0 auto 72px;

	form{
		width: 294px;
		margin: 0 auto;
		overflow: hidden;
	}

		input, textarea{
			width: 294px;
			height: 45px;
			border: none;
			padding: 3px;
			border-radius: 2px;
			display: inline;
			margin-bottom: 12px;
			float: right;
			font-size: 1.8em;
			// color: $light-gray;
		}

		textarea{
			height: 198px;
		}

		input[type=submit]{
			background: none;
			border: none;
			border-radius: 0;
			background: $blue;
			color: $white;
			clear: both;
			height: 45px;
			font-size: 1.8em;
			float: left;
			// margin-left: 9px;
			margin-top: 18px;
		}

		label{
			// margin-left: 10px;
			margin-bottom: 0;
			font-size: 1.8em;
			line-height: 1.6em;
			color: $light-grey;
			font-weight: 400;
			display: inline;
			text-align: right;
			float: left;
		}
	}

.contact-info{
	display: none;
}

footer{
	background: $rich-black;
	height: 54px;
	padding-left: 2%;
	overflow: hidden;

	h1{
		font-size: 2.1em;
		line-height: 54px;
		display: inline-block;
		width: 150px;
		float: left;
	}

	nav{
		display: none;
	}

	p{
		font-size: 1.2em;
		line-height: 54px;
		display: inline-block;
		width: 175px;
		float: right;

	}


}





/*--------------------------------Media Queries------------------------*/
