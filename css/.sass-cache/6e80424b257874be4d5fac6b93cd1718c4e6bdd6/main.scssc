}









/* Desktops and laptops ----------- */
@media only screen 
and (min-width : 837px) {

.scene-inner{
	position: absolute;
	height: 720px;
	left: 0;
	right: 0;
	top: 0;

}

.chandelier{
	width: 862px;
	height: 620px;
	margin: -235px auto 0;
	position: absolute;

}

#curtain-wrapper{
	display: block;
}

img#center-fog{
	height: 620px;
}

.page{
	padding-top: 108px;
	padding-bottom: 108px;
}

.landing.page{
	padding-top: 18px;
	padding-bottom: 0;
}

.ornament{
	margin: -117px auto 90px;
}

.bio{
	.two-col {
       -moz-column-count: 2;
       -moz-column-gap: 20px;
       -webkit-column-count: 2;
       -webkit-column-gap: 20px;
	}
}

.videos{

	.video-content{
		width: 720px;
		margin: 0 auto;
	}

	.video-wrapper{
		float: left;
		

		iframe{
